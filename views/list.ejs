<body>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

<header>
    <%- include('navigation') %>
</header>

<div class="container-fluid m-5">
    <figure>
        <blockquote class="blockquote">
            <h5>Adventure Is Worthwhile</h5>
        </blockquote>
        <figcaption class="blockquote-footer">
            모험은 가치 있는 것이다. <cite title="Source Title"> May </cite>
        </figcaption>
    </figure>
    <div class="row row-cols-3 gap-4">
    <% for (let analysis of data) { %>
        <div class="card shadow p-3 mb-5 bg-body rounded" style="width: 25rem;">
            <button type="button" class="btn-close position-absolute top-0 end-0" aria-label="Close"></button>
            <img  style='height: 250px; width: 100%; object-fit: cover' src="<%=analysis.imageList[0].url%>" class="card-img-top mt-4 " alt="<%= analysis.id %>">
            <div class="card-body">
                <p class="card-text"><%= analysis.memo %></p>
                <p class="card-text"><small class="text-muted">Created at <%= new Date(analysis.regDate).toDateString()%> </small></p>
                <a href="/detail?analysisId=<%=analysis.id%>" class="btn btn-primary">See</a>
            </div>
        </div>
        <% } %>
    </div>
</div>


<script>
